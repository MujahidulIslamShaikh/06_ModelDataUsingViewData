@{
    ViewData["Title"] = "Home Page";
    var emp = ViewData["myEmployee"] as Employee;
    var empViewBag = ViewBag.myEmpData;
    var empTemp = TempData["myEmployee"] as Employee;
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Learn about <a href="https://learn.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>

@if (emp != null)
{


    <h2>@ViewBag.myEmployee.EmpId</h2> 
    <h2>@emp.EmpName</h2>
    <h2>@empViewBag.Designation</h2>
    <h2>@empTemp.Salary</h2>
} else
{
    <h3>Data Not Found!</h3>
}
